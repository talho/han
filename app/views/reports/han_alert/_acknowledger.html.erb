<%- unless acknowledger -%>
  <tr>
    <td>
      <%= acknowledger['display_name'].blank? ? 'None' : acknowledger['display_name'] %>
    </td>
    <td>
      <%= acknowledger['email'].blank? ? 'None' : acknowledger['email'] %>
    </td>
    <td>
      <%= acknowledger['device_type'].blank? ? '' : acknowledger['device_type'] %>
    </td>
    <td>
      <%= acknowledger['response'] %>
    </td>
  </tr>
<%- end -%>




